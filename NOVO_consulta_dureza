SELECT REGEXP_REPLACE(HS_OC.VS_TF_CILINDRO.id_cilindro ,'[[:space:]]{2,}', '') "A", REGEXP_REPLACE(HS_OC.VS_TF_CILINDRO.nm_abrev_tipo_conjunto ,'[[:space:]]{2,}', '') "B", REGEXP_REPLACE(HS_OC.VS_TF_CAMPANHA_CILINDRO.ds_cadeira_laminador ,'[[:space:]]{2,}', '') "C", REGEXP_REPLACE(HS_OC.VS_TF_CILINDRO.cd_laminador ,'[[:space:]]{2,}', '') "D", TO_CHAR(HS_OC.VS_TF_CAMPANHA_CILINDRO.dt_colocacao_laminador,'YYYY-MM-DD HH24:MI:SS') "E", TO_CHAR(HS_OC.VS_TF_CAMPANHA_CILINDRO.dt_saida_laminador,'YYYY-MM-DD HH24:MI:SS') "F", TO_CHAR(HS_OC.VS_TF_USINAGEM_CILINDRO.dt_inicio_usinagem,'YYYY-MM-DD HH24:MI:SS') "G", TO_CHAR(HS_OC.VS_TF_USINAGEM_CILINDRO.dt_fim_usinagem,'YYYY-MM-DD HH24:MI:SS') "H", HS_OC.VS_TF_USINAGEM_CILINDRO.vl_dureza_cil_cen_fim "I", HS_OC.VS_TF_USINAGEM_CILINDRO.vl_dureza_cil_lmo_fim "J", HS_OC.VS_TF_USINAGEM_CILINDRO.vl_dureza_cil_lop_fim "K", HS_OC.VS_TF_USINAGEM_CILINDRO.dm_inicial_prod_usinagem "L", HS_OC.VS_TF_USINAGEM_CILINDRO.dm_final_prod_usinagem "M", (HS_OC.VS_TF_USINAGEM_CILINDRO.dm_inicial_prod_usinagem - HS_OC.VS_TF_USINAGEM_CILINDRO.dm_final_prod_usinagem) "N" FROM HS_OC.VS_TF_CAMPANHA_CILINDRO LEFT JOIN HS_OC.VS_TF_CILINDRO ON HS_OC.VS_TF_CILINDRO.id_cilindro = HS_OC.VS_TF_CAMPANHA_CILINDRO.id_cilindro LEFT JOIN HS_OC.VS_TF_USINAGEM_CILINDRO ON HS_OC.VS_TF_USINAGEM_CILINDRO.id_cilindro = HS_OC.VS_TF_CILINDRO.id_cilindro AND HS_OC.VS_TF_USINAGEM_CILINDRO.dt_colocacao_laminador = HS_OC.VS_TF_CAMPANHA_CILINDRO.dt_colocacao_laminador GROUP BY HS_OC.VS_TF_CILINDRO.id_cilindro, HS_OC.VS_TF_CILINDRO.nm_abrev_tipo_conjunto, HS_OC.VS_TF_CAMPANHA_CILINDRO.ds_cadeira_laminador, HS_OC.VS_TF_CILINDRO.cd_laminador, HS_OC.VS_TF_CAMPANHA_CILINDRO.dt_colocacao_laminador, HS_OC.VS_TF_CAMPANHA_CILINDRO.dt_saida_laminador, HS_OC.VS_TF_USINAGEM_CILINDRO.dt_inicio_usinagem, HS_OC.VS_TF_USINAGEM_CILINDRO.dt_fim_usinagem, HS_OC.VS_TF_USINAGEM_CILINDRO.vl_dureza_cil_cen_fim, HS_OC.VS_TF_USINAGEM_CILINDRO.vl_dureza_cil_lmo_fim, HS_OC.VS_TF_USINAGEM_CILINDRO.vl_dureza_cil_lop_fim, HS_OC.VS_TF_USINAGEM_CILINDRO.dm_inicial_prod_usinagem, HS_OC.VS_TF_USINAGEM_CILINDRO.dm_final_prod_usinagem, (HS_OC.VS_TF_USINAGEM_CILINDRO.dm_inicial_prod_usinagem - HS_OC.VS_TF_USINAGEM_CILINDRO.dm_final_prod_usinagem) HAVING HS_OC.VS_TF_CILINDRO.nm_abrev_tipo_conjunto = 'TRA' AND HS_OC.VS_TF_CILINDRO.cd_laminador = 'LE2' AND HS_OC.VS_TF_CAMPANHA_CILINDRO.dt_colocacao_laminador > TO_DATE('#','YYYY-MM-DD HH24:MI:SS') AND HS_OC.VS_TF_CAMPANHA_CILINDRO.dt_colocacao_laminador < SYSDATE AND HS_OC.VS_TF_USINAGEM_CILINDRO.dm_inicial_prod_usinagem IS NOT NULL AND HS_OC.VS_TF_USINAGEM_CILINDRO.dm_inicial_prod_usinagem - HS_OC.VS_TF_USINAGEM_CILINDRO.dm_final_prod_usinagem > 0 ORDER BY HS_OC.VS_TF_CAMPANHA_CILINDRO.dt_colocacao_laminador
